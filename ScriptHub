local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "Max Ã© gay", HidePremium = false,IntroEnabled = false, SaveConfig = true, ConfigFolder = "SettingsFolder"})

if not game.Loaded then
	game.Loaded:Wait()
end
local plr = game.Players.LocalPlayer
local char = plr.Character or plr.CharacterAdded:Wait()
local hum = char:WaitForChild("Humanoid")

-- Values

_G.UseDefaultWalkSpeed = true
_G.UseNormalJumpPower = true

-- Tabs
-- DBZFS
local DBZFS = Window:MakeTab({
	Name = "DBZFS",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

