-- Misc

local Misc = Window:MakeTab({
	Name = "Misc",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local MiscSection = Misc:AddSection({
	Name = "WalkSpeed"
})

local otherSection = Misc:AddSection({
	Name = "Jump Power"
})

Misc:AddToggle({
	Name = "Remove Shadows",
	Default = false,
	Save = true,
	Callback = function(Value)
		if Value == true then
		game.Lighting.GlobalShadows = false
		else
			game.Lighting.GlobalShadows = true
		end
	end
})

MiscSection:AddToggle({
	Name = "Use Game walkspeed",
	Default = true,
	Callback = function(Value)
		_G.UseDefaultWalkSpeed = Value
	end
})

MiscSection:AddSlider({
	Name = "Walk Speed",
	Min = 0,
	Max = 100000000,
	Default = 16,
	Color = Color3.fromRGB(0,100,255),
	Increment = 1,
	ValueName = "WalkSpeed",
	Callback = function(Value)
		while true do
			if _G.UseDefaultWalkSpeed == false then
				hum.WalkSpeed = Value
			else
				return
			end
			task.wait()
		end
	end    
})

otherSection:AddToggle({
	Name = "Use Game JumpPower",
	Default = true,
	Callback = function(Value)
		_G.UseDefaultWalkSpeed = Value
	end
})

otherSection:AddSlider({
	Name = "Jump",
	Min = 0,
	Max = 100000000,
	Default = 50,
	Color = Color3.fromRGB(0,100,255),
	Increment = 1,
	ValueName = "JumpPower",
	Callback = function(Value)
		while true do
			if _G.UseNormalJumpPower == false then
			hum.JumpPower = Value
			else return end
			task.wait()
		end
	end
})

local Debug = Window:MakeTab({
	Name = "Debug",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Debug:AddButton({
    Name = "Load Debug HUB",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/yofriendfromschool1/debugnation/main/decompilers%20and%20debugging/Debuggers.txt"))()
    end
})

OrionLib:Init()
