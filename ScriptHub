	Name = "BoogaHUB V5",
	Callback = function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/FortniBloxYT1/BOOGA-HUB-V4/main/BOOGA-HUB%20V5.lua"))()
  	end    
})
